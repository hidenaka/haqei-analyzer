    "55_1_JJJ": {
      "pathSig": "JJJ",
      "series": "進→進→進",
      "steps": [
        {
          "hex": 55,
          "line": 1,
          "action": "進",
          "lineText": "好調期に要の相手と会えるので、浮かれず礼を守って要点を詰め、協力の段取りを固めるのが良いです。"
        },
        {
          "hex": 55,
          "line": 2,
          "action": "進",
          "lineText": "華やかさが増し異変の兆しも見えやすい時なので、油断せず情報を整え、見落としをなくすのが良いです。"
        },
        {
          "hex": 55,
          "line": 5,
          "action": "進",
          "lineText": "良い知らせと評価が集まりやすいので、慢心せず成果を共有し、恩恵を適切に配分して信頼を固めるのが良いです。"
        }
      ],
      "finalHex": 55,
      "finalLine": 5,
      "probability": 0.72,
      "meta": {
        "source": "narratives:v1->bridge"
      }
    },
