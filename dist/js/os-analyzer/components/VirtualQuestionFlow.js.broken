    if (element.tagName === 'HAQEI-QUESTION' && !element.shadowRoot) {
      console.warn(`âš ï¸ ${element.dataset.questionId} Web ComponentæœªåˆæœŸåŒ– - å¼·åˆ¶åˆæœŸåŒ–ä¸­...`);
      element.connectedCallback?.();
