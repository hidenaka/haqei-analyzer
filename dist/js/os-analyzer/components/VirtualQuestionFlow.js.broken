    `;
    
    // ã‚¢ã‚¯ãƒ†ã‚£ãƒ–ã‚¯ãƒ©ã‚¹è¿½åŠ 
    element.classList.add('active-question', 'visible');
    
    // ARIAå±æ€§è¨­å®š
    element.setAttribute('aria-hidden', 'false');
