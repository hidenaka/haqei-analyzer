{
  "name": "haqei-vue",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc -b && vite build",
    "preview": "vite preview",
    "prepare": "husky",
    "test": "vitest run",
    "test:unit": "vitest",
    "test:watch": "vitest --watch",
    "test:coverage": "vitest run --coverage",
    "test:crud": "vitest run --reporter=verbose src/tests/useCRUDOperations.test.ts",
    "demo:crud": "tsx src/tests/crud-operations-demo.ts",
    "test:e2e": "echo 'E2E tests not configured yet'",
    "lint": "eslint . --ext .vue,.js,.ts,.tsx --fix",
    "format": "prettier --write src/",
    "migrate:questions": "echo 'Run ts-node src/scripts/migrateQuestions.ts manually'",
    "migrate:to-supabase": "tsx src/scripts/migrateToSupabase.ts",
    "migrate:backup": "tsx src/scripts/migrateToSupabase.ts --backup-only",
    "migrate:restore": "tsx src/scripts/migrateToSupabase.ts --restore",
    "migrate:dry-run": "tsx src/scripts/migrateToSupabase.ts --dry-run"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.53.0",
    "axios": "^1.11.0",
    "chart.js": "^4.5.0",
    "date-fns": "^4.1.0",
    "html2canvas": "^1.4.1",
    "jspdf": "^3.0.1",
    "pinia": "^3.0.3",
    "vue": "^3.5.18",
    "vue-router": "^4.5.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.32.0",
    "@testing-library/vue": "^8.1.0",
    "@types/chart.js": "^2.9.41",
    "@types/node": "^24.1.0",
    "@typescript-eslint/eslint-plugin": "^8.38.0",
    "@typescript-eslint/parser": "^8.38.0",
    "@vitejs/plugin-vue": "^6.0.1",
    "@vitest/ui": "^3.2.4",
    "@vue/test-utils": "^2.4.6",
    "@vue/tsconfig": "^0.7.0",
    "autoprefixer": "^10.4.21",
    "chalk": "^5.0.0",
    "commander": "^12.0.0",
    "eslint": "^9.32.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.3",
    "eslint-plugin-vue": "^10.4.0",
    "husky": "^9.1.7",
    "inquirer": "^12.0.0",
    "jsdom": "^26.1.0",
    "lint-staged": "^16.1.2",
    "ora": "^8.0.0",
    "postcss": "^8.5.6",
    "prettier": "^3.6.2",
    "sass": "^1.89.2",
    "tsx": "^4.20.3",
    "typescript": "~5.8.3",
    "vite": "^7.0.4",
    "vitest": "^3.2.4",
    "vue-tsc": "^2.2.12"
  }
}
