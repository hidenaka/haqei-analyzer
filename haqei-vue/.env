# =====================================================================
# HAQEI Vue 3 開発環境設定 (TASK-035開発版)
# =====================================================================
# 目的: 開発環境でのSupabaseクライアント設定テスト
# 注意: 本番環境では実際のSupabase認証情報を設定してください
# =====================================================================

# === Supabase設定 (開発版) ===
VITE_SUPABASE_URL=https://demo-project.supabase.co
VITE_SUPABASE_ANON_KEY=eyJhbGciOiJIUzI1NiIsInR5cCI6IkpXVCJ9.demo-key

# === データベース接続制御 ===
VITE_DB_CONNECTION_TIMEOUT=5000
VITE_DB_RETRY_ATTEMPTS=2
VITE_DB_RETRY_DELAY=1000

# === Triple OS Architecture制御 ===
VITE_ENABLE_ENGINE_OS=true
VITE_ENABLE_INTERFACE_OS=true
VITE_ENABLE_SAFE_MODE_OS=true

# === 易経64卦システム制御 ===
VITE_ENABLE_ICHING_INTEGRATION=true
VITE_HEXAGRAM_CACHE_TTL=3600000

# === bunenjin哲学・プライバシー制御 ===
VITE_DEFAULT_PRIVACY_LEVEL=maximum
VITE_ENABLE_OFFLINE_MODE=true
VITE_ENABLE_LOCALSTORAGE_FALLBACK=true
VITE_LOCALSTORAGE_PREFIX=haqei_dev_

# === Supabase機能制御 ===
VITE_ENABLE_REALTIME=true
VITE_ENABLE_STORAGE=true
VITE_ENABLE_AUTH=false
VITE_ENABLE_RLS=true

# === Storage設定 ===
VITE_STORAGE_BUCKET=haqei-dev-data
VITE_MAX_FILE_SIZE=10485760
VITE_ALLOWED_FILE_TYPES=pdf,json,csv,png,jpg,jpeg

# === RLS設定 ===
VITE_RLS_CHECK_MODE=strict
VITE_ENABLE_AUDIT_LOG=true
VITE_DATA_RETENTION_DAYS=2555

# === セッション管理 ===
VITE_SESSION_TIMEOUT_MINUTES=30
VITE_AUTO_SAVE_INTERVAL=60000

# === パフォーマンス制御 ===
VITE_ENABLE_PERFORMANCE_MONITORING=true
VITE_CACHE_STRATEGY=memory
VITE_CACHE_TTL=300000

# === 開発環境設定 ===
VITE_DEBUG_MODE=true
NODE_ENV=development