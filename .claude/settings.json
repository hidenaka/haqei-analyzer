{
  "maxFileCharacters": 50000,
  "maxSearchResults": 20,
  "maxConcurrentOperations": 8,
  "taskBatchSize": 5,
  "operationTimeout": 30000,
  "memoryStrategy": "aggressive",
  "cacheStrategy": "extensive",
  "persistentMemory": true,
  "priorityMemoryFiles": [
    ".serena/memories/HAQEI_Project_Master_Plan_Overview.md",
    "cipher-memory/HAQEI_PROJECT_MEMORY_20250805.md",
    ".serena/memories/HAQEI_Phase3_Issues_and_Visual_Development_Rules.md",
    "CLAUDE.md"
  ],
  "compressionStrategy": {
    "enableCompression": true,
    "recentHistoryDays": 7,
    "compressionDelay": "24h",
    "preserveImportantSessions": true,
    "compressionRatio": {
      "recent": 0.1,
      "medium": 0.5,
      "old": 0.8
    }
  },
  "sessionManagement": {
    "keepRecentSessions": 10,
    "compressAfterDays": 3,
    "neverCompressPatterns": [
      "HAQEI",
      "Phase3",
      "記憶",
      "重要",
      "Critical"
    ]
  },
  "disableHeavyOperations": [
    "largeFileAnalysis",
    "deepRecursiveSearch",
    "extensiveCodeGeneration"
  ],
  "performanceOptimizations": {
    "enableLazyLoading": true,
    "limitParallelAgents": 5,
    "reduceMemoryFootprint": true,
    "throttleFileWatchers": true
  }
}